/*月薪宝*/
#pageyxb .pageyxb-header,#pagedjb .pageyxb-header,.pageyxb-monery{
background:#338fff;
}

.pageyxb-header{border:none}
#pagetqb .pageyxb-header h1{font-size:2rem;color:#fff}
#pageyxb .pageyxb-header h1{font-size:2rem;color:#fff}
#pagedjb .pageyxb-header h1{font-size:2rem;color:#fff}
#pagetqb .pageyxb-header h1 a{color:#fff}
#pageyxb .pageyxb-header h1 a{color:#fff}
#pagedjb .pageyxb-header h1 a{color:#fff}
.pageyxb-monery div{
padding:30px 0 20px 0;
text-align:center;
color:#fff
}
.pageyxb-touzi{background:#fff}
.pageyxb-tzjindu{width:90%;height:.5rem;margin:0 auto;background:#fd6721;border-radius:5px}


.pageyxb-news{background:#fff}
.pageyxb-news ul {padding:0 1rem}
.pageyxb-news ul li{padding-bottom:2rem;color:#666}
.pageyxb-news ul li span{float:right}
.pageyxb-footer{background:#afb3b6;padding:1rem;color:#fff;text-align:center;font-size:2rem}

/*积分商城*/
#pagejfsc .jfdh{padding:1rem;background:#fff;color:#000;font-size:1.5rem;font-weight:bold}
.jfdh b{color:#fd6721;}
.jfdh span{float:right;font-size:1.5rem;border:1px solid #338fff;color:#338fff;border-radius:0.5rem}
.jfdh-jiangpin{margin-top:2rem}

.jfsc-hb p{background:#f5f5f5;padding:1rem 0}
.jfsc-hb  img{width:60%}
.jfsc-hb strong{color:#fd6721}

/*商品明细*/
.pagespmx-footer{}
.pagespmx-footer p{padding:1rem;background:#338fff;color:#fff;text-align:center;font-size:1rem;cursor:pointer}
.spmx .ui-grid-a{text-align:left}
.spmx .ui-grid-b ul li{display:inline-block}
.spmx .ui-grid-b ul li input{width:2rem}
.pagespmx-introduct h2{background:#eee}
.pagespmx-hb {text-align:center}
.pagespmx-hb img{width:40%}

/*送红包*/

.tiyan-content{width:90%;margin:0 auto;}
.tiyan{background:#f8c538}
.tiyan-header ul li{width:10%;display:inline-block}
.tiyan-header ul li:nth-child(odd){padding:0.2rem;border:1px solid #fff;height:3rem;border-radius:1.5rem}
.tiyan-header ul li:nth-child(even){border-top:1px solid #fff;height:1.5rem}
.tiyan-body{background:#f1a239;padding:1rem 0;text-align:center}

.tiyan-body img,.tiyan-body button{width:40%}
.tiyan .tiyan-body button{margin:0 auto;background:#b40783;color:#fff}
.tiyan .tiyan-body button:hover{background:#cc66cc;color:#fff}

/*会员特权*/
#pagehytq .img{background:#338fff;padding:2rem 0}
#pagehytq .img img{width:22%}
.pagehytq-content .ui-grid-b{text-align:center}
.pagehytq-content .ui-grid-b div{border:1px solid #eee}
.pagehytq-content .ui-grid-b img{width:50%}

/*安全保障*/
#pageaqbz{color:#fff;background:url("../img/安全保障/images/1.jpg") no-repeat ;background-size:cover}
.pageaqbz-top{font-size:2rem;text-align:center;padding:3rem 1rem}
.pageaqbz-md-div ul{
width:70%;
margin:0 auto;
padding:0;
overflow:hidden;
}
.pageaqbz-md-div ul li{
width:50%;
float:left;
padding:2rem  0;
text-align:center;
}
.pageaqbz-md-div ul li:nth-child(1){background:rgba(233,229,226,0.5)}
.pageaqbz-md-div ul li:nth-child(2){background:rgba(182,226,112,0.5)}
.pageaqbz-md-div ul li:nth-child(3){background:rgba(218,197,98,0.5)}
.pageaqbz-md-div ul li:nth-child(4){background:rgba(102,139,185,0.5)}
.pageaqbz-md-div ul li:hover{background:#fff}
.pageaqbz-bottom strong{color:#ff445b;font-size:2rem}
.pageaqbz-bottom-down{text-align:center;margin-top:5rem;cursor:pointer}
/*项目安全保障*/

#pagexmaqbz .pageaqbz-md img{width:100%}



